pipeline {
    agent any

    environment {
        TF_VERSION = "1.10.5"  // Updated Terraform version
    }

    stages {
        stage('Clone Repository') {
            steps {
                git branch: 'main', url: 'https://github.com/RahulVarne/terraform.git'
            }
        }

        stage('Setup Terraform') {
            steps {
                sh 'terraform --version'
                sh 'terraform init'
            }
        }
    }
}
